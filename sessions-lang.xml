<session name="lang" probability="15" type="ts_http">

  <setdynvars sourcetype="file" fileid="address" delimiter=";" order="random">
    <var name="address" />
  </setdynvars>

  <transaction name="empty">

    <request subst="true">
      <http url="/api/?q=%%_address%%" method="GET" version="1.1"></http>
    </request>
  </transaction>

</session>

