<session name="limitno" probability="25" type="ts_http">

  <setdynvars sourcetype="file" fileid="address" delimiter=";" order="iter">
    <var name="address" />
  </setdynvars>

  <transaction name="limitno">

    <request subst="true"> 
      <http url="/api/?q=%%_address%%" method="GET" version="1.1"></http> 
    </request>    
  </transaction>

</session>

